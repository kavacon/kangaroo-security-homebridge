{
  "pluginAlias": "KangarooSecurity",
  "pluginType": "platform",
  "singular": true,
  "schema": {
    "type": "object",
    "properties": {
      "name": {
        "title": "name",
        "type": "string",
        "required": true,
        "default": "KangarooSecurity"
      },
      "tempStorage": {
        "title": "folder for downloading ephemeral files",
        "type": "string",
        "required": false,
        "default": "/tmp"
      },
      "phone": {
        "title": "phone number registered with kangaroo",
        "type": "string",
        "required": true,
        "default": "+61400000000"
      },
      "security_code": {
        "title": "security code sent to phone number",
        "type": "string",
        "required": true,
        "default": "xxxxxx"
      }
    }
  }
}